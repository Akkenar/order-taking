{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ]
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "lodash",
    "syntax-dynamic-import",
    [
      "transform-react-remove-prop-types",
      {
        "mode": "wrap"
      }
    ]
  ]
}
